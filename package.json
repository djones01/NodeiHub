    "@covalent/http": "^1.0.0-beta.1-1",
    "@covalent/markdown": "^1.0.0-beta.1-1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "@types/body-parser": "^0.0.33",
